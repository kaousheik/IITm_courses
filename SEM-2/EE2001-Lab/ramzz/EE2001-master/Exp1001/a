#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1b5cb30 .scope module, "upcounter_testbench" "upcounter_testbench" 2 1;
 .timescale 0 0;
v0x1b6edb0_0 .var "clk", 0 0;
v0x1b6ee50_0 .net "counter", 1 0, v0x1b6eb90_0; 1 drivers
v0x1b6ef00_0 .var "reset", 0 0;
v0x1b6efb0_0 .var "x", 0 0;
S_0x1b5cc20 .scope module, "dut" "up_counter" 2 4, 3 1, S_0x1b5cb30;
 .timescale 0 0;
v0x1b36f80_0 .var "D", 1 0;
v0x1b6e9a0_0 .var "Dprev", 1 0;
v0x1b6ea40_0 .net "clk", 0 0, v0x1b6edb0_0; 1 drivers
v0x1b6eae0_0 .alias "counter", 1 0, v0x1b6ee50_0;
v0x1b6eb90_0 .var "counter_up", 1 0;
v0x1b6ec30_0 .net "reset", 0 0, v0x1b6ef00_0; 1 drivers
v0x1b6ed10_0 .net "x", 0 0, v0x1b6efb0_0; 1 drivers
E_0x1b5d5d0 .event posedge, v0x1b6ec30_0, v0x1b6ea40_0;
    .scope S_0x1b5cc20;
T_0 ;
    %set/v v0x1b36f80_0, 0, 2;
    %end;
    .thread T_0;
    .scope S_0x1b5cc20;
T_1 ;
    %wait E_0x1b5d5d0;
    %load/v 8, v0x1b36f80_0, 2;
    %set/v v0x1b6e9a0_0, 8, 2;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.0, 4;
    %load/x1p 8, v0x1b6e9a0_0, 1;
    %jmp T_1.1;
T_1.0 ;
    %mov 8, 2, 1;
T_1.1 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v0x1b6ed10_0, 1;
    %inv 9, 1;
    %and 8, 9, 1;
    %load/v 9, v0x1b6e9a0_0, 1; Only need 1 of 2 bits
; Save base=9 wid=1 in lookaside.
    %load/v 10, v0x1b6ed10_0, 1;
    %and 9, 10, 1;
    %add 8, 9, 1;
    %ix/load 0, 1, 0;
    %set/x0 v0x1b36f80_0, 8, 1;
    %load/v 8, v0x1b6e9a0_0, 1; Only need 1 of 2 bits
; Save base=8 wid=1 in lookaside.
    %load/v 9, v0x1b6ed10_0, 1;
    %inv 9, 1;
    %and 8, 9, 1;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.2, 4;
    %load/x1p 9, v0x1b6e9a0_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 9, 2, 1;
T_1.3 ;
; Save base=9 wid=1 in lookaside.
    %inv 9, 1;
    %load/v 10, v0x1b6ed10_0, 1;
    %and 9, 10, 1;
    %add 8, 9, 1;
    %ix/load 0, 0, 0;
    %set/x0 v0x1b36f80_0, 8, 1;
    %load/v 8, v0x1b6ec30_0, 1;
    %jmp/0xz  T_1.4, 8;
    %set/v v0x1b6eb90_0, 0, 2;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v0x1b6ed10_0, 1;
    %jmp/0xz  T_1.6, 8;
    %load/v 8, v0x1b36f80_0, 2;
    %set/v v0x1b6eb90_0, 8, 2;
    %jmp T_1.7;
T_1.6 ;
    %load/v 8, v0x1b6eb90_0, 2;
    %set/v v0x1b6eb90_0, 8, 2;
T_1.7 ;
T_1.5 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1b5cb30;
T_2 ;
    %vpi_call 2 6 "$dumpfile", "test_add.vcd";
    %vpi_call 2 7 "$dumpvars", 1'sb0, S_0x1b5cb30;
    %set/v v0x1b6efb0_0, 0, 1;
    %set/v v0x1b6edb0_0, 0, 1;
    %set/v v0x1b6ef00_0, 0, 1;
    %vpi_call 2 11 "$monitor", "At t=%t, output=%b,x = %b , reset = %b", $time, v0x1b6ee50_0, v0x1b6efb0_0, v0x1b6ef00_0;
    %end;
    .thread T_2;
    .scope S_0x1b5cb30;
T_3 ;
    %delay 5, 0;
    %load/v 8, v0x1b6edb0_0, 1;
    %inv 8, 1;
    %set/v v0x1b6edb0_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1b5cb30;
T_4 ;
    %set/v v0x1b6efb0_0, 1, 1;
    %set/v v0x1b6ef00_0, 1, 1;
    %delay 10, 0;
    %set/v v0x1b6ef00_0, 0, 1;
    %delay 10, 0;
    %set/v v0x1b6efb0_0, 0, 1;
    %delay 10, 0;
    %set/v v0x1b6efb0_0, 1, 1;
    %delay 10, 0;
    %set/v v0x1b6efb0_0, 0, 1;
    %delay 10, 0;
    %set/v v0x1b6efb0_0, 1, 1;
    %delay 80, 0;
    %set/v v0x1b6efb0_0, 0, 1;
    %vpi_call 2 23 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test.v";
    "dff.v";
